<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

     <managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
     <managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.UserBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>sessionBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.SessionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.MessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>navigationBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.NavigationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>categorieListBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.CategorieListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>categorieBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.CategorieBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>productListBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.ProductListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>productBean</managed-bean-name>
		<managed-bean-class>com.pinhost.app.webshopxxl2.beans.ProductBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	
</faces-config>